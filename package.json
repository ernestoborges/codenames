{
  "name": "codenames",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node --loader ts-node/esm --experimental-specifier-resolution=node src/server.ts",
    "build-dev": "tsc && next build",
    "build-prod": "pnpm install --frozen-lockfile; tsc && next build && ls dist",
    "start": "node --loader ts-node/esm --experimental-specifier-resolution=node --max_old_space_size=400 ./dist/server.js",
    "lint": "next lint"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "firebase": "^10.13.1",
    "firebase-admin": "^12.5.0",
    "jsonwebtoken": "^9.0.2",
    "next": "14.2.7",
    "pnpm": "^9.9.0",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.3.0",
    "sharp": "^0.33.5",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "ts-node": "^10.9.2",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.7",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
